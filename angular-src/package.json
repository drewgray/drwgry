{
    "name": "angular-src",
    "version": "0.0.0",
    "license": "MIT",
    "angular-cli": {},
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "test": "ng test",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor",
        "clean": "rm -rf ../public",
        "build": "npm run clean && tsc -p tsconfig.json",
        "build_prod": "npm run clean && ng build && npm run minify && npm run gz",
        "minify": "uglifyjs ../public/inline.bundle.js --screw-ie8 --compress --mangle --output ../public/inline.bundle.min.js && uglifyjs ../public/main.bundle.js --screw-ie8 --compress --mangle --output ../public/main.bundle.min.js && uglifyjs ../public/polyfills.bundle.js --screw-ie8 --compress --mangle --output ../public/polyfills.bundle.min.js && uglifyjs ../public/styles.bundle.js --screw-ie8 --compress --mangle --output ../public/styles.bundle.min.js && uglifyjs ../public/vendor.bundle.js --screw-ie8 --compress --mangle --output ../public/vendor.bundle.min.js",
        "gz": "gzip ../public/inline.bundle.min.js && gzip ../public/main.bundle.min.js && gzip ../public/polyfills.bundle.min.js && gzip ../public/styles.bundle.min.js && gzip ../public/vendor.bundle.min.js"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^2.3.1",
        "@angular/compiler": "^2.3.1",
        "@angular/core": "^2.3.1",
        "@angular/forms": "^2.3.1",
        "@angular/http": "^2.3.1",
        "@angular/platform-browser": "^2.3.1",
        "@angular/platform-browser-dynamic": "^2.3.1",
        "@angular/router": "^3.3.1",
        "angular2-flash-messages": "^1.0.5",
        "angular2-jwt": "^0.1.28",
        "angular2-modal": "^2.0.3",
        "core-js": "^2.4.1",
        "instafeed.js": "^1.4.1",
        "multer": "^1.3.0",
        "ng2-bootstrap": "^1.3.3",
        "ng2-file-upload": "^1.2.0",
        "ng2-google-charts": "^1.0.1",
        "rxjs": "^5.0.1",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.7.2"
    },
    "devDependencies": {
        "@angular/compiler-cli": "^2.3.1",
        "@types/jasmine": "2.5.38",
        "@types/node": "^6.0.42",
        "angular-cli": "1.0.0-beta.28.3",
        "codelyzer": "~2.0.0-beta.1",
        "jasmine-core": "2.5.2",
        "jasmine-spec-reporter": "2.5.0",
        "karma": "1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-remap-istanbul": "^0.2.1",
        "protractor": "~4.0.13",
        "ts-node": "1.2.1",
        "tslint": "^4.3.0",
        "typescript": "~2.0.3"
    }
}